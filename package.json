{
  "name": "soliverchallenge",
  "version": "1.0.0",
  "description": "S. Oliver Challenger Webshop",
  "scripts": {
    "start": "concurrently \"npm run start:vue\" \"npm run start:node\"",
    "start:vue": "cd app && npm run dev",
    "start:node": "cd server && node server.js",
    "install-dependencies": "npm install && npm run install:subfolders",
    "install:subfolders": "cd app && npm install && cd ../server && npm install"
  },
